@page
@using ReservationGuest.Components

@model Datemodel
@{
    ViewData["Title"] = "Datum";
}

    <script type="text/javascript">
        $(function () {
            $('.month-picker').datetimepicker({
                viewMode: 'months',
                format: 'MM/YYYY',
                useCurrent: false
            });
        });
    </script>
<script src="_framework/blazor.server.js"></script>

@(await Html.RenderComponentAsync<Datum>(RenderMode.ServerPrerendered))
